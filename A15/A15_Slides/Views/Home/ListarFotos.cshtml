@model IEnumerable<TP16_Slides.Models.Foto>
@{
    ViewData["Title"] = "Listar Fotos";
    Layout = "~/Views/Shared/_MyLayout.cshtml";
}
<div class="lista-de-fotos">
    @if (Model.Count() == 0)
    {
        <p> Desculpe. não foram encontradas fotografias</p>
    }
    else
    {
        @: Lista de fotografias disponíveis:
        foreach (var foto in Model)
        {
            <br />

            <b>@foto.Codigo -- @foto.Titulo</b>
        }
    }
</div>